<mapper namespace="dao.AdministratorMapper">


    <select id="getAdministrator" parameterType="String" resultType="administrator" >
      select admin_user_name as userName,admin_user_pwd as passWord from admin_user where
      admin_user_name=#{userName}
    </select>
